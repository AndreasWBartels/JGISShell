    </layer>
  </folder>

  <folder>
    <name>Open Railway Map</name>
    <layer>
      <name>Max Speed</name>
      <url><![CDATA[osm:http://{identifier}.tiles.openrailwaymap.org/maxspeed/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
    </layer>
    <layer>
      <name>Standard</name>
      <url><![CDATA[osm:http://{identifier}.tiles.openrailwaymap.org/standard/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
    </layer>
  </folder>

  <folder>
    <name>Open Infrastructure Map</name>
    <layer>
      <name>petroleum</name>
      <url><![CDATA[osm:https://tiles-{identifier}.openinframap.org/petroleum/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
    </layer>
    <layer>
      <name>telecoms</name>
      <url><![CDATA[osm:https://tiles-{identifier}.openinframap.org/telecoms/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
    </layer>
    <layer>
      <name>water</name>
      <url><![CDATA[osm:https://tiles-{identifier}.openinframap.org/water/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
    </layer>
  </folder>

  <folder>
    <name>Way Marked Trails</name>
    <layer>
      <name>Cycling</name>
      <url><![CDATA[osm:http://tile.waymarkedtrails.org/cycling/{zoomLevel}/{column}/{row}.png]]></url>
    </layer>
    <layer>
      <name>Hiking</name>
      <url><![CDATA[osm:http://tile.waymarkedtrails.org/hiking/{zoomLevel}/{column}/{row}.png]]></url>
    </layer>
    <layer>
      <name>MTB</name>
      <url><![CDATA[osm:http://tile.waymarkedtrails.org/mtb/{zoomLevel}/{column}/{row}.png]]></url>
    </layer>
  </folder>

  <folder>
    <name>Aster</name>
    <layer>
      <name>Aster contour lines</name>
      <url><![CDATA[osm:http://korona.geog.uni-heidelberg.de/tiles/asterc/x={column}&y={row}&z={zoomLevel}]]></url>
    </layer>
    <layer>
      <name><![CDATA[Aster GDEM & SRTM]]></name>
      <url><![CDATA[osm:http://korona.geog.uni-heidelberg.de/tiles/asterh/x={column}&y={row}&z={zoomLevel}?minimumZoomLevel=14&maximumZoomLevel=18]]></url>
    </layer>
  </folder>

  <folder>
    <name>Feeds</name>
    <layer>
      <name>Recent Earthquacks</name>
      <url><![CDATA[georss:http://quakes.bgs.ac.uk/feeds/WorldSeismology.xml]]></url>
    </layer>
    <layer>
      <name><![CDATA[Download Service BGR]]></name>
      <url><![CDATA[georss:https://services.bgr.de/atomfeeds/service.xml]]></url>
    </layer>
    <layer>
      <name><![CDATA[SERTIT - Activations]]></name>
      <url><![CDATA[georss:http://sertit.u-strasbg.fr/RMS/georss/ActivationsSertit.xml]]></url>
    </layer>
    <layer>
      <name><![CDATA[DLR/ZKI Emergency Mapping Activations]]></name>
      <url><![CDATA[georss:https://www.zki.dlr.de/iwg-sem/feeds/activations]]></url>
    </layer>
    <layer>
      <name><![CDATA[Copernicus EMS Risk and Recovery Mapping Activations]]></name>
      <url><![CDATA[georss:http://emergency.copernicus.eu/mapping/activations-risk-and-recovery/feed]]></url>
    </layer>
    <layer>
      <name><![CDATA[Copernicus EMS Rapid Mapping Activations]]></name>
      <url><![CDATA[georss:http://emergency.copernicus.eu/mapping/activations-rapid/feed]]></url>
    </layer>
    <layer>
      <name><![CDATA[Geonames]]></name>
      <url><![CDATA[georss:http://www.geonames.org/recent-changes.xml]]></url>
    </layer>
    <layer>
      <name><![CDATA[Geonetwork]]></name>
      <url><![CDATA[georss:http://www.fao.org/geonetwork/srv/en/rss.latest]]></url>
    </layer>
    <layer>
      <name><![CDATA[UNESCO World Heritage List]]></name>
      <url><![CDATA[georss:http://whc.unesco.org/en/list/georss/]]></url>
    </layer>
  </folder>

  <layer>
    <name>OSM Streets (de)</name>
    <url><![CDATA[osm:http://{identifier}.tile.openstreetmap.de/tiles/osmde/{zoomLevel}/{column}/{row}.png?identifiers=a,b,c,d]]></url>
  </layer>
  <layer>
    <name>Max Speed</name>
    <url><![CDATA[osm:http://wince.dentro.info/koord/osm/tiles/{zoomLevel}/{column}/{row}.png]]></url>
  </layer>
  <layer>
    <name>Public transport</name>
    <url><![CDATA[osm:http://pt.openmap.lt/{zoomLevel}/{column}/{row}.png]]></url>
  </layer>
  <layer>
    <name>Maptoolkit Terrain</name>
    <url><![CDATA[osm:https://tile{identifier}.maptoolkit.net/terrain/{zoomLevel}/{column}/{row}.png?identifiers=1,2,3,4]]></url>
  </layer>

</configuration>
